package com.hyva.restopos.rest.pojo;

public class InvoicePayment {
    private Long paymentType;
    private double totalINVAmountRefunded;
    private double totalINVDiscount;
    private double invoicePayAmt;
    private String invmobile;
    private String invmail;
    private String paymentName;


    public Long getPaymentType() {
        return paymentType;
    }

    public void setPaymentType(Long paymentType) {
        this.paymentType = paymentType;
    }

    public double getTotalINVAmountRefunded() {
        return totalINVAmountRefunded;
    }

    public void setTotalINVAmountRefunded(double totalINVAmountRefunded) {
        this.totalINVAmountRefunded = totalINVAmountRefunded;
    }

    public double getTotalINVDiscount() {
        return totalINVDiscount;
    }

    public void setTotalINVDiscount(double totalINVDiscount) {
        this.totalINVDiscount = totalINVDiscount;
    }

    public double getInvoicePayAmt() {
        return invoicePayAmt;
    }

    public void setInvoicePayAmt(double invoicePayAmt) {
        this.invoicePayAmt = invoicePayAmt;
    }

    public String getInvmobile() {
        return invmobile;
    }

    public void setInvmobile(String invmobile) {
        this.invmobile = invmobile;
    }

    public String getInvmail() {
        return invmail;
    }

    public void setInvmail(String invmail) {
        this.invmail = invmail;
    }

    public String getPaymentName() {
        return paymentName;
    }

    public void setPaymentName(String paymentName) {
        this.paymentName = paymentName;
    }
}
