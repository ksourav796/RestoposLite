<!DOCTYPE html>
<html lang="en">
<style>
    .table>thead:first-child>tr:first-child>th {
        height: 38px !important;
    }
</style>
<body>
<div class="container-fluid">
    <div class="container cus_main_row">

        <div class="row">
            <h2 class="pos-header-new">
                <div>Configuration</div>
            </h2>
        </div>
        <div class="row inner_row_bg">
            <div class="col-md-12" id="save_new_AccountSetup_modal">
                <div class="panel">
                    <div class="panel-body">
                        <div class="col-md-6 col-md-offset-3 col-sm-offset-2 col-sm-8">
                            <div class="form-horizontal ng-pristine ng-valid">
                                <div class="form-group">

                                    <label class="control-label col-lg-3 col-sm-4" for="">Cash Account :</label>
                                    <div class="col-lg-9 col-sm-8" style="position:relative;">
                                        <input type="hidden" ng-model="CashAccountIDText">
                                        <input type="text"
                                               class="form-control input-md"
                                               placeholder="Select Account"
                                               ng-model="account_name"
                                               ng-click="getCashAccountList(accountSearchText)">
                                    </div>

                                </div>
                                <div class="form-group">
                                    <label class="control-label col-lg-3 col-sm-4" for="">Bank Account :</label>
                                    <div class="col-lg-9 col-sm-8">
                                        <input type="hidden" ng-model="BankAccountIDText">
                                        <input type="text" class="form-control input-md"
                                               placeholder="Select Account"
                                               ng-model="BankAccountText"
                                               ng-click="getBankAccountList(accountSearchText)"
                                               style="width: 77%; display: inline;">

                                        <a class="btn btn-default add_btn" ng-click="addNewbankPopulate()" style="display: inline;
                                                  padding: 8px; background: dimgray; color: #fff;"><i class="fa fa-plus fa-fw"></i></a>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="control-label col-lg-3 col-sm-4" for="">Unit Price:</label>
                                    <div class="col-lg-9 col-sm-8">
                                        <select ng-model="UnitPriceText" class="form-control">
                                            <option value="Editable"> Editable
                                            <option value="Non-editable"> Non-editable
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-lg-3 col-sm-4" for="">Discount:</label>
                                    <div class="col-lg-9 col-sm-8">
                                        <select ng-model="DiscountText" class="form-control">
                                            <option value="Editable"> Editable
                                            <option value="Non-editable"> Non-editable
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group" ng-show="DiscountText=='Editable'">
                                    <label class="control-label col-lg-3 col-sm-4" for="">Discount Type:</label>
                                    <div class="col-lg-9 col-sm-8">
                                        <input type="radio" ng-model="discountType" value="itemWise">Item Wise
                                        <input type="radio" ng-model="discountType" value="billWise">Bill Wise
                                    </div>
                                </div>
                                <div class="form-group" ng-show="discountType=='billWise'">
                                    <label class="control-label col-lg-3 col-sm-4" for="">Tax Percent:</label>
                                    <div class="col-lg-9 col-sm-8">
                                        <input type="checkbox" ng-model="taxcheckbox">
                                    </div>
                                    <div class="col-lg-9 col-sm-8">
                                        <select class="form-control input-md"
                                                ng-model="taxid"
                                                ng-options="tax.taxId as tax.taxName for tax in taxList">
                                            <option value="">Select</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group" ng-show="DiscountText=='Editable'">
                                    <label class="control-label col-lg-3 col-sm-4" for="">Sort Orders:</label>
                                    <div class="col-lg-9 col-sm-8">
                                        <input type="radio" ng-model="sortOrder" value="Asc">Asc
                                        <input type="radio" ng-model="sortOrder" value="Desc">Desc
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-lg-3 col-sm-4" for="">PompOrder TimeOut :</label>
                                    <div class="col-lg-9 col-sm-8" style="position:relative;">
                                        <input type="text" maxlength="3"
                                               class="form-control input-md"
                                               placeholder="pompOrderTimeOut"
                                               ng-model="pompOrderTimeOut">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-lg-3 col-sm-4" for="">Print Type:</label>
                                    <div class="col-lg-9 col-sm-8">
                                        <input type="radio" ng-model="PrintTypeText" value="normal">Normal (A4)
                                        <input type="radio" ng-model="PrintTypeText" value="normalA5">Normal (A5)
                                        <input type="radio" ng-model="PrintTypeText" value="pos">POS (3 Inch)
                                        <br/>
                                        <input type="radio" ng-model="PrintTypeText" value="normalwithshippaddr">NormalWithShippingAddress
                                        <input type="radio" ng-model="PrintTypeText" value="printeddata">PrePrintedData
                                        <input type="radio" ng-model="PrintTypeText" value="preprintedwithshippaddr">PrePrintedDataWithShippingAddress
                                    </div>
                                </div>
                                <div class="form-group">
                                    <fieldset data-ng-repeat="field in posPrinterModalList ">
                                        <label class="control-label col-lg-3 col-sm-4" for="">POS Printer :</label>
                                        <div class="col-lg-9 col-sm-8">
                                            <select ng-model="field.posPrinter" class="form-control"
                                                    ng-options="value as value for (key , value) in printerDeviceList"
                                                    style="width: 25%; display: inline;">
                                            </select>
                                            ||
                                            <select ng-model="field.user" class="form-control"
                                                    ng-options="user.full_name as
											user.full_name for user in userlist"  ng-disabled="$index == 0" style="width: 25%;display: inline;">
                                            </select>
                                            ||
                                            <select ng-model="field.tableConfig" class="form-control"
                                                    ng-options="table.tableconfigid as
											table.configurationname for table in tableConfigurationList"  ng-disabled="$index == 0" style="width: 20%; display: inline">
                                            </select>
                                            <a class="btn btn-default add_btn" ng-click="addPosPrinterModel()" style="display: inline;
                                            padding: 8px; background: dimgray; color: #fff; display: inline"><i class="fa fa-plus fa-fw"></i></a>

                                            <a class="btn btn-default add_btn" ng-click="removePosPrinter($index)" style="display: inline;
                                            padding: 8px; background: dimgray; color: #fff; display: inline"><i class="fa fa-minus fa-fw"></i></a>
                                        </div>
                                    </fieldset>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-lg-3 col-sm-4" for="">Print Preview:</label>
                                    <div class="col-lg-9 col-sm-8" ng-init="printPreview=No">
                                        <input type="radio" ng-model="printPreview" value="Yes">Yes
                                        <input type="radio" ng-model="printPreview" value="No">No
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-lg-3 col-sm-4" for="">Notification:</label>
                                    <div class="col-lg-9 col-sm-8" ng-init="notification=No">
                                        <input type="radio" ng-model="notification" value="Yes">Yes
                                        <input type="radio" ng-model="notification" value="No">No
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="control-label col-lg-3 col-sm-4" for="">Restaurant:</label>
                                    <div class="col-lg-9 col-sm-8">
                                        <input type="radio" ng-model="Restaurant"  value="DineIn" ng-init="Restaurant='dineIn'">Dine In
                                        <input type="radio" ng-model="Restaurant" value="TakeAway">Take Away
                                        <input type="radio" ng-model="Restaurant" value="Delivery">Delivery
                                        <input type="radio" ng-model="Restaurant" value="onlineDelivery">Online Delivery
                                        <input type="radio" ng-model="Restaurant" value="digiOrders">Digi Orders
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="control-label col-lg-3 col-sm-4" for="">Cancelled KOT (Printer):</label>
                                    <div class="col-lg-9 col-sm-8">
                                        <input type="checkbox" ng-model="reportToReceipt" ng-init="reportToReceipt=true">Receipt Printer
                                        <input type="checkbox" ng-model="reportToCategory" ng-init="reportToCategory=false">Category Printer
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="control-label col-lg-3 col-sm-4" for="">Prompt KOT (Printer):</label>
                                    <div class="col-lg-9 col-sm-8">
                                        <input type="checkbox" ng-model="promptKOT" ng-init="promptKOT=false">Yes / No
                                    </div>
                                </div>

                                <div class="form-group">
                                    <fieldset data-ng-repeat="field in kotPrinterList track by $index">
                                        <label class="control-label col-lg-3 col-sm-4" for="">Kot Printer :</label>
                                        <div class="col-lg-9 col-sm-8">
                                            <select ng-model="kotPrinterList[$index]" class="form-control"
                                                    ng-options="value as value for (key , value) in printerDeviceList"
                                                    style="width: 45%; display: inline;">
                                            </select>
                                            ||
                                            <select ng-model="categoryList[$index]" class="form-control"
                                                    ng-options="value.itemCategoryName as value.itemCategoryName for (key , value) in itemCategoryList"
                                                    style="width: 25%; display: inline;" ng-disabled="$index == 0">
                                            </select>


                                            <a class="btn btn-default add_btn" ng-click="addKOT()" style="display: inline;
                                            padding: 8px; background: dimgray; color: #fff;"><i class="fa fa-plus fa-fw"></i></a>

                                            <a class="btn btn-default add_btn" ng-click="removeKOT($index)" style="display: inline;
                                            padding: 8px; background: dimgray; color: #fff;"><i class="fa fa-minus fa-fw"></i></a>
                                        </div>
                                    </fieldset>
                                </div>

                                <div class="form-group">
                                    <label class="control-label col-lg-3 col-sm-4" for="">A4 Printer</label>
                                    <div class="col-lg-9 col-sm-8" style="position:relative;">
                                        <select ng-model="cnfa4printer" class="form-control"
                                                ng-options = "value as value for (key , value) in printerDeviceList">
                                        </select>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <fieldset data-ng-repeat="field in kotPrinterModelList track by $index">
                                        <label class="control-label col-lg-3 col-sm-4" for="">Printer Display :</label>
                                        <div class="col-lg-9 col-sm-8">
                                            <select ng-model="kotPrinterModelList[$index]" class="form-control"
                                                    ng-options="value as value for (key , value) in printerDeviceList"
                                                    style="width: 45%; display: inline;">
                                            </select>
                                            ||
                                            <select ng-model="printModelList[$index]" class="form-control"
                                                    style="width: 25%; display: inline;">
                                                <option value="TVSRP3150"> TVS RP3150
                                                <option value="EPSONTMT82"> EPSON TM T-82
                                                <option value="GENERIC58MM"> GENERIC 58MM
                                            </select>


                                            <a class="btn btn-default add_btn" ng-click="addPrinterModel()" style="display: inline;
                                            padding: 8px; background: dimgray; color: #fff;"><i class="fa fa-plus fa-fw"></i></a>

                                            <a class="btn btn-default add_btn" ng-click="removePrinterModel($index)" style="display: inline;
                                            padding: 8px; background: dimgray; color: #fff;"><i class="fa fa-minus fa-fw"></i></a>
                                        </div>
                                    </fieldset>
                                </div>

                                <div class="form-group">
                                    <label class="control-label col-lg-3 col-sm-4" for="">Take Away Printer</label>
                                    <div class="col-lg-9 col-sm-8" style="position:relative; width: 75%">
                                        <select ng-model="mobilebillprinter" class="form-control"
                                                ng-options = "value as value for (key , value) in printerDeviceList">
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <fieldset data-ng-repeat="field in kotPrinterModelListMobile track by $index">
                                        <label class="control-label col-lg-3 col-sm-4" for="">Printer Display Mobile:</label>
                                        <div class="col-lg-9 col-sm-8">
                                            <select ng-model="kotPrinterModelListMobile[$index]" class="form-control"
                                                    ng-options="value as value for (key , value) in printerDeviceList"
                                                    style="width: 45%; display: inline;">
                                            </select>
                                            ||
                                            <select ng-model="printModelListMobile[$index]" class="form-control"
                                                    style="width: 25%; display: inline;">
                                                <option value="TVSRP3150"> TVS RP3150
                                                <option value="EPSONTMT82"> EPSON TM T-82
                                                <option value="GENERIC58MM"> GENERIC 58MM
                                            </select>
                                        </div>
                                    </fieldset>
                                </div>
                                <div class="form-group">
                                    <label class="control-label col-lg-3 col-sm-4" for="">Printer Selection</label>
                                    <div class="col-lg-9 col-sm-8" style="position:relative; width: 75%">
                                        <input id="printerSelection" name="printerSelection"
                                               type="text" placeholder="printerSelection" ng-click="openPrinterSelectionPage()"
                                               ng-model="printerSelection1"
                                               class="form-control input-md" autocomplete="off">
                                    </div>
                                </div>



                            </div>
                        </div>
                        <div class="col-md-12">
                            <div class="form-horizontal ng-pristine ng-valid">
                            </div>
                            <div class="form-group">
                                <div class="col-sm-6 pull-right">
                                    <button type="submit" class="btn btn-default" data-toggle="modal" ng-disabled="isDisabled"
                                            data-target=".print-me"
                                            ng-click="saveAccountSetup('save_new_AccountSetup_modal')">Save
                                    </button>
                                </div>
                            </div>

                        </div>

                    </div>
                </div>
            </div>

            <div class="col-md-12 col-sm-12">
                <div class="fixed-table-container">
                    <div class="table_wrap fixed-table-container-inner">
                        <div class="header-background"></div>
                        <table data-toggle="table" data-cache="false" data-height="299" style="margin-bottom: 0;"
                               class="hi_table table table-hover table-bordered table-striped table-fixed-header">
                            <thead>
                            <tr>
                                <th data-field="cntryname">
                                    <div class="th-inner">Cash Account</div>
                                </th>
                                <th data-field="cntryname">
                                    <div class="th-inner">Bank Account</div>
                                </th>
                                <th data-field="cntryname">
                                    <div class="th-inner">Unit Price</div>
                                </th>
                                <th data-field="cntryname">
                                    <div class="th-inner">Discount</div>
                                </th>

                            </tr>
                            </thead>
                            <tbody id="parentTable">
                            <tr ng-repeat="accountSetup in configurationList| filter:searchText">

                                <td>{{accountSetup.cashAccountName}}</td>
                                <td>{{accountSetup.bankAccountName}}</td>
                                <td>{{accountSetup.unitPrice}}</td>
                                <td>{{accountSetup.discount}}</td>
                                <!--<td><a id="edit" ng-click="editConfiguratPopulate(bank)" class="edit"><i class="fa fa-pencil-square-o fa-fw orange"></i></a></td>-->
                            </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>


            <div class="modal fade selectItem" tabindex="-1" id="selectAccount" role="dialog" data-backdrop="static"
                 aria-labelledby="mySmallModalLabel">
                <div class="modal-dialog modal-md">
                    <div class="modal-content checkout">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                                    aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title">Select Account</h4>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <label>Search: <input class="form-control input-sm"
                                                          ng-change="getCashAccountList(searchText)"
                                                          ng-change-delay="300" ng-model="searchText"></label>
                                    <div class="table-responsive">
                                        <table class="table table-striped table_costomize itemlisttable four_table_info"
                                               id="">
                                            <thead>
                                            <tr>
                                                <th data-field="name">Account Code</th>
                                                <th data-field="desc">Account Name</th>
                                                <th data-field="desc">Upper Level Name</th>
                                                <th data-field="desc">Account Type</th>
                                                <!--<th data-field="desc">Item Category Name</th>-->
                                            </tr>
                                            </thead>
                                            <tbody id="parentTable">
                                            <tr ng-repeat="account in accountList| filter:searchText"
                                                ng-click="showAcceptedNotificationList ? updateNotificationItemInList(account) : addAccount(account,$index);">
                                                <td hidden>{{account.accountid}}</td>
                                                <td>{{account.stringAccountCode}}</td>
                                                <!--<td class="word_break">{{account.account_name}}</td>-->
                                                <td>{{account.account_name}}</td>
                                                <td>{{account.parentAccount_name}}</td>
                                                <td>{{account.accountGroup}}</td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal fade selectItem" tabindex="-1" id="selectAccount1" role="dialog" data-backdrop="static"
                 aria-labelledby="mySmallModalLabel">
                <div class="modal-dialog modal-md">
                    <div class="modal-content checkout">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                                    aria-hidden="true" ng-click="remove()">&times;</span></button>
                            <h4 class="modal-title">Select Account</h4>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-12">
                                    <label>Search: <input class="form-control input-sm"
                                                          ng-keyup="$event.keyCode == 13 ? getBankAccountList(text) : null"
                                                          ng-change-delay="300" ng-model="text"></label>
                                    <div class="table-responsive">
                                        <table class="table table-striped table_costomize itemlisttable four_table_info"
                                               id="">
                                            <thead>
                                            <tr>
                                                <th data-field="name">Account Code</th>
                                                <th data-field="desc">Account Name</th>
                                                <th data-field="desc">Upper Level Name</th>
                                                <th data-field="desc">Account Type</th>
                                                <!--<th data-field="desc">Item Category Name</th>-->
                                            </tr>
                                            </thead>
                                            <tbody id="parentTable">
                                            <tr ng-repeat="account in accountList| filter:searchText"
                                                ng-click="showAcceptedNotificationList ? updateNotificationItemInList(account) : addAccount1(account,$index);">
                                                <td hidden>{{account.accountid}}</td>
                                                <td>{{account.stringAccountCode}}</td>
                                                <!--<td class="word_break">{{account.account_name}}</td>-->
                                                <td>{{account.account_name}}</td>
                                                <td>{{account.parentAccount_name}}</td>
                                                <td>{{account.accountGroup}}</td>
                                            </tr>
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" ng-click="remove()" class="btn btn-default" data-dismiss="modal">
                                Close
                            </button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal fade ExchangeRate" tabindex="-1" id="add_new_Bank_modal" role="dialog"
                 data-backdrop="static"
                 aria-labelledby="mySmallModalLabel">
                <div class="modal-dialog modal-md">
                    <div class="modal-content checkout">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                                    aria-hidden="true">&times;</span></button>
                            <h4 id="bank-title" class="modal-title">Add Bank</h4>
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-md-12 col-sm-12">
                                    <form name="addBank" class="form-horizontal">
                                        <fieldset>
                                            <div class="form-group">
                                                <label class="col-md-4 col-sm-4 control-label" for="AccountNo">Account
                                                    No</label>
                                                <div class="col-md-6 col-sm-6" style="position:relative;">
                                                    <input id="accountNo" name="accountNo" alphanum-with-space
                                                           minlength="5" maxlength="25"
                                                           type="text" placeholder="Account No"
                                                           ng-model="accountNoText"
                                                           ng-blur="hideAutoComplete()"
                                                           class="form-control input-md" autocomplete="off" required>
                                                    <span style="color:red" ng-show="addBank.AccountNo.$error.pattern">Minimum length should be 5 Digits</span>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-md-4 col-sm-4 control-label" for="">Bank Name</label>
                                                <div class="col-md-6 col-sm-6" style="position:relative;">
                                                    <input id="bankName" name="bankName" maxlength="25"
                                                           allowpattern="[a-z]"
                                                           type="text" placeholder="Bank Name"
                                                           ng-model="bankNameText"
                                                           ng-model-options="{ debounce: 1000 }"
                                                           ng-blur="hideAutoComplete()"
                                                           class="form-control input-md" autocomplete="off" required>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-md-4 col-sm-4 control-label" for="BranchName">Branch
                                                    Name</label>
                                                <div class="col-md-6 col-sm-6" style="position:relative;">
                                                    <input id="branchName" name="branchName" maxlength="125"
                                                           allowpattern="[a-z]"
                                                           type="text" placeholder="Branch Name"
                                                           ng-model="BranchNameText"
                                                           ng-model-options="{ debounce: 1000 }"
                                                           ng-blur="hideAutoComplete()"
                                                           class="form-control input-md" autocomplete="off" required>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="col-md-4 col-sm-4 control-label"
                                                       for="Address">Address</label>
                                                <div class="col-md-6 col-sm-6" style="position:relative;">
                        <textarea id="Address" name="Address" maxlength="255"
                                  allowpattern="[a-z]"
                                  type="text" placeholder="Address"
                                  ng-model="AddressText"
                                  ng-model-options="{ debounce: 1000 }"
                                  ng-blur="hideAutoComplete()"
                                  class="form-control input-md"> </textarea>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="col-md-4 col-sm-4 control-label" for="PhoneNo">Phone
                                                    No</label>
                                                <div class="col-md-6 col-sm-6" style="position:relative;">
                                                    <input id="PhoneNo" name="PhoneNo"
                                                           type="text" placeholder="Phone No"
                                                           ng-model="PhoneNoText" Num-with-space
                                                           maxlength="10" ng-minlength="10"
                                                           class="form-control input-md" autocomplete="off" required>
                                                </div>
                                            </div>
                                            <div class="form-group">
                                                <label class="col-md-4 col-sm-4 control-label" for="Email">Email</label>
                                                <div class="col-md-6 col-sm-6" style="position:relative;">
                                                    <input id="Email" name="Email"
                                                           type="text" placeholder="Email"
                                                           ng-model="EmailText"
                                                           ng-pattern="word"
                                                           ng-blur="hideAutoComplete()"
                                                           class="form-control input-md" autocomplete="off" required>
                                                    <p class="error" ng-show="addBank.Email.$error.pattern">Enter Valid
                                                        MailID</p>
                                                </div>
                                            </div>

                                            <div class="form-group">
                                                <label class="col-md-4 col-sm-4 control-label" for="IFSCCode">IFSC
                                                    Code</label>
                                                <div class="col-md-6 col-sm-6" style="position:relative;">
                                                    <input id="IFSCCode" name="IFSCCode"
                                                           allowpattern="[a-z]"
                                                           type="text" placeholder="IFSC Code"
                                                           ng-model="IFSCCodeText" maxlength="20"
                                                           ng-model-options="{ debounce: 1000 }"
                                                           ng-blur="hideAutoComplete()"
                                                           class="form-control input-md" autocomplete="off" required>
                                                </div>
                                            </div>
                                            <div class="form-group">
