        <script>
            count = 0;
            $(document).on("click", ".accordion-toggle", function () {
                bef_attId = $(this).attr("data-target");
                if ($('tr[class="accordion-toggle"].list').not('.collapsed').length > 1) {
                    $('tr[class="accordion-toggle"].list').not('.collapsed').each(function () {
                        if ($(this).attr("data-target") !== bef_attId) {
                            var id = $(this).attr("data-target").substr(1);
                            var element = document.getElementById($('#' + id + ' div > ul> li').attr('id'));
                            //angular.element( element ).scope().$destroy();
                        }
                    });
                }
                if ($(this).find(".glyphicon").hasClass('glyphicon-minus-sign')) {
                    $(this).find(".glyphicon").removeClass('glyphicon-minus-sign').addClass('glyphicon');
                } else if ($(this).find(".glyphicon").hasClass('glyphicon')) {
                    $(this).find(".glyphicon").removeClass('glyphicon').addClass('glyphicon-minus-sign');
                }
                //    $(this).siblings('tr[class="accordion-toggle"].list').not('.collapsed').click();
            });
        </script>

