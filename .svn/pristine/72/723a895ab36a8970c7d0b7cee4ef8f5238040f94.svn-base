                    <div class="col-md-2 col-sm-2" style="padding: 0 3px;width: 180px">
                        <select ng-model="configurationname" ng-change="getAllTables(configurationname,'','config')"
                               id="configurationname"
                                ng-options="tableconfig.configurationname as tableconfig.configurationname for tableconfig in tableConfigurationList"
                               class="form-control input-md"
                               style="border-radius: 0px; border: 1px solid green !important;  text-align: center;width: 87%">
                            <option value="">Select TableConfig</option>
                        </select>
                    </div>
                    <div class="col-md-2 col-sm-2" style="padding: 0 3px;width: 180px" ng-show="configurationname!=null" >
                        <div class="pad">
                            <select
                                   ng-model="tableSearchText" ng-change="getAllTables(configurationname,tableSearchText,'table')"
                                   id="iDTableSearchText" ng-disabled="table"
                                    ng-options="table.tableName as table.tableName for table in completeTableList"
                                    class="form-control input-md"
                                   style="border-radius: 0px; border: 1px solid green !important;  text-align: center;">
                                <option value="">Select Table</option>
                            </select>
                        </div>
                    </div>
                </div>

                <div class="col-lg-8 col-md-9 col-sm-12">
                    <button type="button" class="btn btn-danger" ng-show="userAccessRights.restDineIn" >DINE IN</button>
                    <button type="button" class="btn btn-danger" ng-click="setOrderType('TakeAway')" ng-show="userAccessRights.restTakeaway">TAKE AWAY</button>
                    <button type="button" class="btn btn-danger" ng-click="setOrderType('Delivery')" ng-show="userAccessRights.restDelivery">DELIVERY</button>
                    <button type="button" class="btn btn-danger" ng-click="openOnlinePage('OnlineDelivery')" ng-show="userAccessRights.restOnlineDelivery">ONLINE DELIVERY</button>
                    <button type="button" class="btn btn-danger" ng-click="openTableReservation('tableReserve')" ng-show="userAccessRights.restTableReservation">TABLE RESERVATION</button>
                    <button type="button" class="btn btn-danger" ng-click="openDigiOrders('digiorders')" ng-show="userAccessRights.restDigiOrders">DIGI ORDERS</button>
                </div>
            </div>

            <div class="row table_div" style="height: 450px; overflow: auto;">
                <div class="col-md-12 col-sm-12">
                    <div class="col-md-12 col-sm-12" style="padding:3px;">
                        <div class="col-md-2 col-sm-2" style="padding:3px;min-height: 160px !important; max-height: 160px !important;" ng-repeat="table in tableList">
                            <div>
                                <div ng-show="table.message" class="blink-one">
                                    <div class="col-md-12 col-sm-12"
                                         style="color: black; text-align: justify; padding:0;min-height: 90px"
                                         ng-class='{red : table.tableStatus=="Occupied",yellow:table.tableStatus=="CustomerBill",green : table.tableStatus=="Empty",blue:table.tableStatus=="Reserved"}'
                                         ng-click="appendTable(table)">
                                        <div class="col-md-12" style="padding: 0px;" ng-show="table.waiterName">
                                            <p style="float: right; font-size: 12px; margin: 0px;">{{table.waiterName}}
                                                <i class="fa fa-user" aria-hidden="true"></i>&nbsp;</p>
                                        </div>
                                        <h3 style="display:inline-block; font-size: 18px !important; width:94%;text-align: center">
                                            {{table.orderTime|date:'hh:mm'}}</h3>
                                        <h3 style="display:inline-block; font-size: 18px !important; width:96%;text-align: center">
                                            {{table.tableName}}</h3>
                                    </div>
                                </div>
                                <div ng-hide="table.message">
                                    <div class="col-md-12 col-sm-12"
                                         style="color: black; text-align: justify; padding:0;min-height: 90px"
                                         ng-class='{red : table.tableStatus=="Occupied",yellow:table.tableStatus=="CustomerBill",green : table.tableStatus=="Empty",blue:table.tableStatus=="Reserved"}'
                                         ng-click="appendTable(table)">
                                        <div class="col-md-12" style="padding: 0px;" ng-show="table.waiterName">
                                            <p style="float: right; font-size: 12px; margin: 0px;">{{table.waiterName}}
                                                <i class="fa fa-user" aria-hidden="true"></i>&nbsp;</p>
                                        </div>
                                        <h3 style="display:inline-block; font-size: 18px !important; width:94%;text-align: center">
                                            {{table.orderTime|date:'hh:mm'}}</h3>
                                        <h3 style="display:inline-block; font-size: 18px !important; width:96%;text-align: center">
                                            {{table.tableName}}</h3>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<restaurant-print></restaurant-print>
<payment-modal-restaurant></payment-modal-restaurant>
