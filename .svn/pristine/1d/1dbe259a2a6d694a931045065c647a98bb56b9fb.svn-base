<div class="modal fade voucherPayment" tabindex="-1" id="advancePosPayment" role="dialog" data-backdrop="static"
     aria-labelledby="mySmallModalLabel">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                        aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Payment</h4>
            </div>
            <div class="modal-body" style="height: 460px; overflow-y: scroll;">
                <form class="form-horizontal" name="multi">
                    <fieldset>
                        <div class="row total_info">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="Total">Total</label>
                                    <div class="col-md-8">
                                        <input id="totalVPBeforDiscount" name="totalVPBeforDiscount"
                                               ng-model="totalVPBeforDiscount"
                                               type="number" readonly="true" class="form-control input-md">
                                    </div>
                                </div>
                            </div>
                            <input id="amountWithoutDiscount" ng-model="amountWithoutDiscount"
                                   name="amountWithoutDiscount"
                                   readonly="true" type="hidden" class="form-control input-md">
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="Writeoff Amount">Discount</label>
                                    <div class="col-md-8">
                                        <input id="totalVPDiscount" name="totalVPDiscount"
                                               ng-model="totalVPDiscount"

                                               ng-readonly="userRights.discount== false"
                                               ng-change="OnChangeMultiDiscount({{totalVPDiscount}})" maxlength="8"
                                               type="text" class="form-control input-md">
                                    </div>
                                </div>
                            </div>
                            <!--<div class="col-md-6">-->
                            <!--<div class="form-group">-->
                            <!--<label class="col-md-4 control-label" for="Payment Amount">Payment Amount</label>-->
                            <!--<div class="col-md-8">-->
                            <input id="totalVPAfterDiscount" ng-model="totalVPAfterDiscount"
                                   name="totalVPAfterDiscount"
                                   readonly="true" type="hidden" class="form-control input-md">
                            <!--</div>-->
                            <!--</div>-->
                            <!--</div>-->
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="Payment Amount">Amount
                                        Tendered</label>
                                    <div class="col-md-8">
                                        <input id="totalVPAfterDiscount" ng-model="totalPaidAmt" name="totalPaidAmt"
                                               readonly="true" type="text" class="form-control input-md">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="Amount Refunded">Amount
                                        Returned</label>
                                    <div class="col-md-8">
                                        <input id="totalVPAmountRefunded" name="totalVPAmountRefunded" readonly=""
                                               ng-model="totalVPAmountRefunded"
                                               class="form-control input-md">
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-md-offset-1 col-md-10 Payment_checkbox">
                                <label class="checkbox-inline"><input type="checkbox" ng-true-value="'cashPayment'"
                                                                      ng-model="cashcheck"
                                                                      ng-change="getClearPaymentList(cashcheck,'cash')"
                                                                      id="cash_ch">Cash</label>
                                <label class="checkbox-inline"><input type="checkbox" ng-model="card_checked"
                                                                      ng-true-value="'creditPayment'"
                                                                      ng-change="CardEntity(card_checked); getClearPaymentList(card_checked,'card')"
                                                                      id="card_ch">Card</label>
                                <label class="checkbox-inline"><input type="checkbox" ng-model="voucher_checked"
                                                                      ng-true-value="'voucherPayment'"
                                                                      ng-change="vouchersEntity(voucher_checked); getClearPaymentList(voucher_checked,'voucher')"
                                                                      id="voucher_ch">Voucher</label>
                            </div>
                        </div>

                        <div class="card_info  row" ng-show="cashcheck">
                            <div class="col-md-12"
                                 style="margin: 10px 0px 9px 0px; font-size: 14px; font-weight: bold;">
                                <i class="fa fa-money" style="  font-size: 17px; color: #138bf5;"></i> Cash Payment
                            </div>
                        </div>

                        <div class="form-group" id="cash_info" ng-show="cashcheck">
                            <label class="col-md-4 control-label" for="Amount tendered">Amount tendered</label>
                            <div class="col-md-5">
                                <input id="totalCPAmountTendered" ng-model="totalCPAmountTendered"
                                       name="totalVPAmountTendered" type="number"

                                       ng-click="getSelectedTexElement($event)"
                                       ng-change="OnChangeMultiPayament({{totalVPAfterDiscount}}, totalCPAmountTendered)"
                                       class="form-control input-md" required/>
                            </div>
                        </div>

                        <!-- Card info Start  here -->
                        <div class="card_info  row" ng-show="card_checked">
                            <div class="col-md-12"
                                 style="margin: 10px 0px 9px 0px; font-size: 14px; font-weight: bold;">
                                <i class="fa fa-credit-card" style="  font-size: 17px; color: #138bf5;"></i> Card
                                Payment
                            </div>
                        </div>

                        <div class="card_info  row" id="card_info_1" data-ng-repeat="card in cards"
                             myattr={{$index}} ng-show="card_checked">
                            <div class="col-md-5 TransactionNo_info">
                                <div class="form-group">
                                    <label class="col-md-4 control-label">Transaction No</label>
                                    <div class="col-md-8">
                                        <input id="transcNo_1" ng-model="card.transactionNo"
                                               name="transactionNumber"
                                               type="text" class="form-control input-md transcNo">
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-5 cardAmount_info">
                                <div class="form-group">
                                    <label class="col-md-4 control-label">Card Amount</label>
                                    <div class="col-md-8">
                                        <input id="cardAmt_1" ng-model="card.cardAmount" name="cardAmt"
                                               ng-click="getSelectedTexElement($event)"
                                               ng-change="OnChangeMultiPayament({{totalVPAmountTendered}}, card.cardAmount)"
                                               type="number" class="form-control input-md cardAmt"
                                               required/>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-2 cardAmount_info button_info">
                                <button class="btn btn-default voucher_plus remove" ng-hide="$first"
                                        ng-click="removeCard($index)">-
                                </button>
                                <button class="btn btn-default voucher_plus addfields" ng-show="$last"
                                        ng-click="addNewCard()">+
                                </button>
                            </div>


                            <!-- Card info End here -->

                            <div class="voucher_info  row" ng-show="voucher_checked">
                                <div class="col-md-12"
                                     style="margin: 10px 0px 9px 0px; font-size: 14px; font-weight: bold;">
                                    <i class="fa fa-credit-card" style="  font-size: 17px; color: #138bf5;"></i> Voucher
                                    Payment
                                </div>
                            </div>

                            <div class="voucher_info row" id="voucher_info_1" data-ng-repeat="voucher in vouchers"
                                 myattr={{$index}} ng-show="voucher_checked">
                                <div class="col-md-5 voucher_no_info">
                                    <div class="form-group">
                                        <label class="col-md-4 control-label" for="Voucher No">Voucher No</label>
                                        <div class="col-md-8">
                                            <input id="voucherNo_1" ng-model="voucher.voucherNo" name="voucherNo"
                                                   type="text" size="9" maxlength="8"
                                                   class="form-control input-md voucher_No">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-5 voucher_amount_info">
                                    <div class="form-group">
                                        <label class="col-md-4 control-label" for="Voucher Amount">Voucher
                                            Amount</label>
                                        <div class="col-md-8">
                                            <input id="totalVoucherAmt_1" ng-model="voucher.amt" name="totalVoucherAmt"
                                                   ng-click="getSelectedTexElement($event)"
                                                   ng-change="OnChangeMultiPayament({{totalVPAfterDiscount}},voucher.amt,hiddenVoucherAmt)"
                                                   type="number"
                                                   class="form-control input-md Voucher_Amount" ng-required="true"/>
                                            <input type="hidden" ng-model="hiddenVoucherAmt">
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-2 voucher_amount_info button_info">
                                    <button class="btn btn-default add_btn voucher_plus remove" ng-hide="$first"
                                            ng-click="removeVoucher($index)">-
                                    </button>
                                    <button class="btn btn-default add_btn voucher_plus addfields" ng-show="$last"
                                            ng-click="addNewVoucher()">+
                                    </button>
                                </div>
                            </div>


                            <div ng-show="showEmailBox" class="form-group">
                                <label class="col-md-4 control-label" for="Amount Refunded">Email Id</label>
                                <div class="col-md-5">
                                    <input id="asd" name="emailId" ng-model="customerEmail"
                                           class="form-control input-md">
                                </div>
                            </div>
                    </fieldset>
                    <div class="modal-footer">
                        <button type="submit" class="btn btn-default" data-toggle="modal"
                                ng-click="saveMultiAdvancePaySI()">Save & Print
                        </button>
                        <!--<button type="submit" class="btn btn-default" data-toggle="modal"-->
                        <!--ng-click="saveSIandEmail('multiPayment')">Save & Email-->
                        <!--</button>-->
                        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                    </div>
                </form>
            </div>

        </div>
    </div>
</div>
<div class="modal fade selectPaymentForReturn bs-check" tabindex="-1" id="selectPaymentForReturn" role="dialog"
     data-backdrop="static" aria-labelledby="mySmallModalLabel">
