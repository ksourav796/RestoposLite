<div class="modal fade printreceipt" tabindex="-1" id="printreceipt" role="dialog" data-backdrop="static"
     aria-labelledby="mySmallModalLabel">
    <div class="modal-dialog modal-lg">
        <div class="modal-content checkout">
            <div id="printablearea" class="panel-body" class="mini-print-wrapper">
                <div class="row">
                    <div class="col-md-12">
                        <div class="table-responsive ">
                            <table class="resipt_wrap">
                                <tr>
                                    <td>
                                        <table cellpadding="0" cellspacing="0" style="width:100%;">
                                            <tr>
                                                <img style="width:40px;height:40px;" src="{{companyLogoPath}}">
                                            </tr>
                                            <tr>
                                                <td colspan="3"
                                                    style="text-align: center; text-transform: uppercase; font-size: 16px; padding-top: 5px; padding-bottom: 18px;">
                                                    <strong>{{printData.companyData.company_name}}</strong>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td colspan="3" style="font-size: 11px;">
                                                    <strong>{{inventoryadress}}</strong>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="font-size: 11px; padding-bottom: 12px; vertical-align: top">
                                                    aaaa :
                                                    {{printData.inventoryLocationData.InventoryLocationContact}}
                                                </td>
                                                <td>&nbsp;</td>
                                                <td style="font-size: 11px; padding-bottom: 12px; vertical-align: top">
                                                    zzzz :
                                                    {{printData.inventoryLocationData.InventoryLocationFaxNo}}
                                                </td>
                                            </tr>
                                            <tr>
                                                <td colspan="3"><span style="color:#000">Gst Reg No. :</span> <span
                                                        style="display: inline-block; border-bottom: 1px dotted #0E0E0E;">{{printData.companyData.registerNo}}</span>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td colspan="3"><span style="color:#000">Tax Invoice No. :</span>
                                                    <span style="display: inline-block; border-bottom: 1px dotted #0E0E0E;">{{printData.siData.siNo}}</span>
                                                </td>
                                                <td colspan="3"><span style="color:#000">Serial Invoice No. :</span>
                                                    <span style="display: inline-block; border-bottom: 1px dotted #0E0E0E;">{{printData.siData.srlnNo}}</span>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="color:#000; padding-top: 9px; padding-bottom: 12px;">
                                                    Date: {{printData.date}}
                                                </td>
                                                <td style="color:#000; padding-top: 9px; padding-bottom: 12px;">
                                                    &nbsp;
                                                </td>
                                                <td style="color:#000; padding-top: 9px; padding-bottom: 12px;">
                                                    {{printData.time}}
                                                </td>
                                            </tr>
                                            <tr>
                                                <td style="font-size: 11px; padding-bottom: 12px; vertical-align: top"
                                                    colspan="3">
                                                    Customer Name :<b>{{printData.siData.cutomerName}}</b>
                                                </td>
                                            </tr>
                                        </table>
                                        <table cellpadding="0" cellspacing="0" style="width: 100%;">
                                            <thead>
                                            <tr>
                                                <th style="text-align: left;" valign="top">Item</th>
                                                <!--<!&#45;&#45;<th style="text-align: left" valign="top">Desc</th>&#45;&#45;>-->
                                                <th style="text-align: center" valign="top">Unit Price <br/> RM</th>
                                                <th style="text-align: center" valign="top">Unit</th>
                                                <th style="text-align: center" valign="top">Serial No</th>
                                                <th style="text-align: center" valign="top">Total(RM)</th>
                                                <th style="text-align: center" valign="top"></th>
                                            </tr>
                                            </thead>
                                            <tbody id="parentTable">
                                            <tr ng-repeat="item in printData.siData.selectedAccountList">
                                                <td style="word-break: break-all; width: 43px;" valign="top">
                                                    {{item.itemName}} {{item.itemDescription}}</td>
                                                <td style="text-align: center" valign="top">{{item.unitPrice}}</td>
                                                <td style="text-align: center" valign="top"> {{item.qty}}</td>
                                                <td style="text-align: left; width: 56px !important; word-break: break-all;"
                                                    valign="top"> {{item.serializableNumbers}}
                                                </td>
                                                <td style="text-align: center" valign="top">
                                                    {{item.amtexclusivetax}}
                                                </td>
                                                <td style="text-align: center" valign="top">{{item.taxName}}</td>
                                            </tr>
                                            <tr>
                                                <td colspan="7"
                                                    style="border-bottom: 1px dashed #000; line-height: 0px; padding-bottom: 0px;">
                                                    &nbsp;
                                                </td>
                                            </tr>
                                            </tbody>
                                        </table>
                                        <table cellpadding="0" cellspacing="0" style="width:100%;">
                                            <tr>
                                                <td colspan="2">&nbsp;</td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    Item Count
                                                </td>
                                                <td align="right">
                                                    {{printData.siData.itemCount}}
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    Total Sales inclusive GST @6%
                                                </td>
                                                <td align="right">
                                                    {{printData.siData.totalCheckOutamt| setDecimal:2}}
                                                </td>
                                            </tr>
                                            <tr>
                                                <td>
                                                    Rounding Adjustment
                                                </td>
                                                <td align="right">
                                                    0.00
                                                </td>
                                            </tr>
                                        </table>
                                        <div ng-switch="printData.siData.paymentType">
                                            <div ng-switch-when="cashPayment">
                                                <table cellpadding="0" cellspacing="0" style="width:100%;">
                                                    <tr>
                                                        <td>
                                                            Cash
