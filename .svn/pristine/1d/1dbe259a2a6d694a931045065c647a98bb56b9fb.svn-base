<div class="modal fade selectpayment bs-check" tabindex="-1" id="selectpayment" role="dialog" data-backdrop="static"
     aria-labelledby="mySmallModalLabel">
    <div class="modal-dialog modal-md">
        <div class="modal-content checkout">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                        aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Select Payment Method</h4>
            </div>
            <div class="modal-body checkout-body">
                <div class="col-md-4 payment-icon">
                    <button type="button" data-toggle="modal" data-dismiss="modal" ng-click="openCashpayment();"><i
                            class="fa fa-money"></i>Cash
                    </button>
                </div>
                <div class="col-md-4 payment-icon">
                    <button type="button" data-toggle="modal" data-dismiss="modal"
                            ng-click="openCreditCardPayment();"><i class="fa fa-credit-card"></i>Credit
                    </button>
                </div>
                <div class="col-md-4 payment-icon">
                    <button type="button" data-toggle="modal" ng-click="openVoucherpayment()" data-dismiss="modal">
                        <i class="fa fa-credit-card"></i>Voucher
                    </button>
                </div>
                <div class="col-md-4 payment-icon">
                    <button type="button" data-toggle="modal" ng-click="openMultipayment()" data-dismiss="modal"><i
                            class="fa fa-credit-card"></i>Multi Payment
                    </button>
                </div>


                <div class="col-md-4 payment-icon">
                    <button type="button" data-toggle="modal" ng-click="openpayment()" data-dismiss="modal"><i
                            class="fa fa-credit-card"></i>Multi Payment
                    </button>
                </div>


            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
<div class="modal fade selectAccount" tabindex="-1" id="selectAccount" role="dialog" data-backdrop="static"
     aria-labelledby="mySmallModalLabel">
    <div class="modal-dialog modal-md">
        <div class="modal-content checkout">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                        aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Select Account</h4>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-12">
                        <label>Search: <input class="form-control input-sm" ng-change="getAccountList(searchText, true)"
                                              ng-change-delay="300" ng-model="searchText"></label>
                        <div class="table-responsive">
                            <table class="table table-striped table_costomize itemlisttable four_table_info" id="">
                                <thead>
                                <tr>
                                    <th data-field="name">Account Code</th>
                                    <th data-field="desc">Account Name</th>
                                    <th data-field="desc">Upper Level Name </th>
                                    <th data-field="desc">Account Type</th>
                                    <!--<th data-field="desc">Item Category Name</th>-->
                                </tr>
                                </thead>
                                <tbody id="parentTable">
                                <tr ng-repeat="account in accountList| filter:searchText"
                                    ng-click="showAcceptedNotificationList ? updateNotificationItemInList(account) : addAccount(account,$index);">
                                    <td hidden>{{account.accountid}}</td>
                                    <td>{{account.stringAccountCode}}</td>
                                    <td class="word_break">{{account.accountname}}</td>
                                    <td>{{account.accountGroup}}</td>
                                    <td>{{account.parentAccountname}}</td>
                                </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
