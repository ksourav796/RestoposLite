                                                                <div class="col-md-6 text-center" style="padding:0px">

                                                                    <table class="table table-striped table-bordered nowrap">
                                                                        <tr style="padding-top:20px!important;">
                                                                            <td style="width: 37%;"><b>Customer Name</b>
                                                                            </td>
                                                                            <td style="width: 3%;">:</td>
                                                                            <td>{{notification.customerName}}</td>
                                                                            <td style="width: 37%;"><b>Amount</b></td>
                                                                            <td style="width: 3%;">:</td>
                                                                            <td>{{notification.amount}}</td>
                                                                            <td>
                                                                                <button type="button"
                                                                                        class="btn btn-success"
                                                                                        width="60" height="100"
                                                                                        ng-click="viewdetails(notification)">
                                                                                    View
                                                                                </button>
                                                                            </td>

                                                                        </tr>

                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </a>
                                    </li>
                                </ul>
                            </li>

                        </ul>
                    </li>
                    <!-- Tasks: style can be found in dropdown.less -->
                    <li class="dropdown notifications-menu">
                        <a class="dropdown-toggle" data-toggle="dropdown">
                            <i class="fa fa-flag-o"></i>
                            <span class="label label-warning"
                                  ng-if="digiplacedList.length">{{digiplacedList.length}}</span>
                        </a>
                        <ul class="dropdown-menu" style="width: 600px;">
                            <li class="header">You have {{digiplacedList.length}} notifications</li>
                            <li>
                                <!-- inner menu: contains the actual data -->
                                <ul class="menu" ng-if="digiplacedList.length">
                                    <li ng-repeat="notification in digiplacedList">
                                        <a>
                                            <div class="row" style="margin: 0px;">
                                                <div class="col-md-3 text-center"
                                                     ng-show="notification.fromCompname=='Zomato'||notification.fromCompname=='zomato'">
                                                    <img src="resource/images/Zomato_logo.png"
                                                         style="width:48px; height:48px; "/>
                                                    <p style="margin: 10px 0 0;color: dimgray;font-weight: 60; border: 1px solid;padding: 0 5px;">
                                                        {{notification.typeFlag}}
                                                    </p>
                                                </div>
                                                <div class="col-md-3 text-center"
                                                     ng-show="notification.fromCompname=='Swiggy'||notification.fromCompname=='swiggy'">
                                                    <img src="resource/images/swiggy-logo.png"
                                                         style="width:48px; height:48px; "/>
                                                    <p style="margin: 10px 0 0;color: dimgray;font-weight: 60; border: 1px solid;padding: 0 5px;">
                                                        {{notification.typeFlag}}
                                                    </p>
                                                </div>
                                                <div class="col-md-3 text-center"
                                                     ng-show="notification.fromCompname=='ubereats'||notification.fromCompname=='UberEats'">
                                                    <img src="resource/images/uber.png"
                                                         style="width:48px; height:48px; "/>
                                                    <p style="margin: 10px 0 0;color: dimgray;font-weight: 60; border: 1px solid;padding: 0 5px;">
                                                        {{notification.typeFlag}}
                                                    </p>
                                                </div>
                                                <div class="col-md-3 text-center"
                                                     ng-show="notification.fromCompname=='Restopos'||notification.fromCompname=='Restopos'">
                                                    <img src="resource/images/favicon-16x16.png"
                                                         style="width:48px; height:48px; "/>
                                                    <p style="margin: 10px 0 0;color: dimgray;font-weight: 60; border: 1px solid;padding: 0 5px;">
                                                        {{notification.typeFlag}}
                                                    </p>
                                                </div>
                                                <div class="col-m`d-3 text-center"
                                                     ng-show="notification.fromCompname=='FoodPanda'||notification.fromCompname=='foodpanda'">
                                                    <img src="resource/images/foodpanda.png"
                                                         style="width:48px; height:48px; "/>
                                                    <p style="margin: 10px 0 0;color: dimgray;font-weight: 60; border: 1px solid;padding: 0 5px;">
                                                        {{notification.typeFlag}}
                                                    </p>
                                                </div>

                                                <div class="col-md-9">
                                                    <div ng-if="notification.status=='placed'">
                                                        <div class="col-md-12" style="padding-left:0px">
                                                            <div class="col-md-5" style="padding:0px">
                                                                <div class="col-md-6 text-center" style="padding:0px">

                                                                    <table class="table table-striped table-bordered nowrap">
                                                                        <tr style="padding-top:20px!important;">
                                                                            <td style="width: 37%;"><b>Customer Name</b>
                                                                            </td>
                                                                            <td style="width: 3%;">:</td>
                                                                            <td>{{notification.customerName}}</td>
                                                                            <td style="width: 37%;"><b>Amount</b></td>
                                                                            <td style="width: 3%;">:</td>
                                                                            <td>{{notification.amount}}</td>
                                                                            <td>
                                                                                <button type="button"
                                                                                        class="btn btn-success"
                                                                                        width="60" height="100"
                                                                                        ng-click="viewdetails(notification)">
                                                                                    View
                                                                                </button>
                                                                            </td>
                                                                        </tr>
                                                                    </table>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                            </div>
                                        </a>
                                    </li>


                                </ul>
                            </li>

                        </ul>
                    </li>

                    <!-- User Account: style can be found in dropdown.less -->
                    <li class="dropdown user user-menu">
                        <a class="dropdown-toggle" data-toggle="dropdown">
                            <!--<img src="dist/img/user2-160x160.jpg" class="user-image" alt="User Image">-->
                            <span class="hidden-xs">{{userName}}</span>
                        </a>
                        <ul class="dropdown-menu">
                            <li class="user-footer">
                                <div class="pull-left">
                                    <a class="btn btn-default btn-flat" ng-click="deactiveLicense()">Deactivate</a>
                                </div>
                                <div class="pull-right">
                                    <a href="#" class="btn btn-default btn-flat">Sign out</a>
                                </div>
                            </li>
                        </ul>
                    </li>
                    <!-- Control Sidebar Toggle Button -->

                </ul>
            </div>
        </nav>
    </header>

    <aside id="side" class="main-sidebar">
        <section class="sidebar">
            <ul class="sidebar-menu" data-widget="tree">
                <li class="active" id="restdashboard" ng-show="userAccessRights.restDashboard">
                    <a id="restdashboardLink" href="#!/restdashboard">
                        <i class="fa fa-dashboard"></i> <span>Dashboard</span>
                    </a>
                </li>
                <li id="restaurantZone" ng-show="userAccessRights.restaurant">
                    <a href="#!/restaurantZone" id="restaurantZoneLink">
                        <i class="fa fa-dashboard"></i> <span>Restaurant</span>
                    </a>
                </li>
                <li class="treeview" ng-show="userAccessRights.tokens">
                    <a>
                        <i class="fa fa-cutlery"></i>
                        <span>Token</span>
                        <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
                    </a>
                    <ul class="treeview-menu">
                        <li><a id="kitchentokenLink" ng-show="userAccessRights.kitchenTokens" href="#!/kitchenTokenRecord"> Kitchen Token </a></li>
                        <li><a id="waitertokenLink" ng-show="userAccessRights.waiterTokens" href="#!/waiterTokenRecord"> Waiter Token </a></li>
                    </ul>
                </li>

                <li ng-show="userAccessRights.masters"><a id="mastersLink" href="#!/masters"><i class="fa fa-cogs"></i> <span>Masters</span></a>
                </li>
                <li class="treeview" ng-show="userAccessRights.finance">
                    <a>
                        <i class="fa fa-money"></i>
                        <span>Finance</span>
                        <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
                    </a>
                    <ul class="treeview-menu">
                        <li><a id="receipt" href="#!/gtforsales" ng-show="userAccessRights.receipt"> Receipt</a></li>
                        <li><a id="expense" href="#!/gtforpurchase" ng-show="userAccessRights.expense"> Expense</a></li>
                        <li><a id="journalentry" href="#!/journalentry" ng-show="userAccessRights.journalEntry"> Journal Entry </a></li>
                        <li><a id="plreport" href="#!/plreport" ng-show="userAccessRights.pandlreport"> Profit &amp; Loss Report</a></li>
                        <li><a id="balanceSheet" href="#!/balanceSheetReport" ng-show="userAccessRights.restaurant"> Balance Sheet Report</a></li>
                    </ul>
                </li>
                <li><a href="#!/company" ng-show="userAccessRights.companyInfo"><i class="fa fa-dashboard"></i> <span>Company Info</span></a>
                </li>
                <li class="treeview" ng-show="userAccessRights.reports">
                    <a>
                        <i class="fa fa-money"></i>
                        <span>Reports</span>
                        <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
                    </a>
                    <ul class="treeview-menu">
                        <li><a href="#!/restaurantReports" ng-show="userAccessRights.restaurantReports"> Restaurant </a></li>
                        <!--<li><a href="#"> Inventory </a></li>-->
                    </ul>
                </li>
            </ul>
        </section>
        <!-- /.sidebar -->
    </aside>
    <div class="modal fade printreceiptA4" tabindex="-1" id="viewalldetail" role="dialog"
         data-backdrop="static"
         aria-labelledby="mySmallModalLabel">
        <div class="modal-dialog modal-lg" style="width:770px;height:842px;">
            <div class="modal-content checkout">
                <div id="receiptPaymentPritableArea" class="panel-body" class="mini-print-wrapper"
                     style=" max-height:500px;overflow-y: auto;">
                    <button type="button" class="close" data-dismiss="modal" style="color: black !important;">x</button>
                    <table style="width:700px;border:1px solid gray;">
                        <tr>
                            <td style="padding: 0 10px">
                                <table class="hi_table">
                                    <tr>
                                        <td>
                                            <table class="hi_table"
                                                   style="width: 700px;border-bottom: 1px solid black;">
                                                <tr>
                                                    <td>
                                                        <div class="invoice-title">
                                                            <h3 style="text-align: center;font-weight: 600;">
                                                                {{notification.restaurantName}} Order Details</h3>
                                                        </div>
                                                    </td>
                                                </tr>
                                            </table>

                                            <table style="width: 700px; border-bottom: 1px solid black; padding-bottom: 10px !important;">
                                                <tr>
                                                    <td>
                                                        <h><b>Customer Details</b></h>
                                                        <table style="width:350px;" class="hi_table">

                                                            <tr style="padding-top:20px!important;">
                                                                <td style="width: 37%;">Customer Name</td>
                                                                <td style="width: 3%;">:</td>
                                                                <td>{{notification.customerName}}</td>
                                                            </tr>
                                                            <tr>
                                                                <td style="width: 37%;">Location</td>
                                                                <td style="width: 3%;">:</td>
                                                                <td>{{notification.location}}</td>

                                                            </tr>
                                                            <tr>
                                                                <td>Phone Number</td>
                                                                <td>:</td>
                                                                <td>{{notification.phone}}</td>
                                                            </tr>
                                                            <tr>
                                                                <td>Aggregator</td>
                                                                <td>:</td>
                                                                <td>{{notification.fromCompname}}</td>
                                                            </tr>
                                                        </table>
                                                    </td>
                                                    <td>
                                                        <table style="width:350px;" class="hi_table">
                                                            <tr style="padding-bottom:20px;">
                                                                <td style="width: 37%;">OrderId</td>
                                                                <td style="width: 3%;">:</td>
                                                                <td>{{notification.notificationId}}</td>
                                                            </tr>
                                                            <tr style="padding-bottom:20px;">
                                                                <td style="width: 37%;">Order Date&Time</td>
                                                                <td style="width: 3%;">:</td>
                                                                <td>{{notification.date|date:'dd/MM/yyyy hh:mm:ss'}}
                                                                </td>
                                                            </tr>
                                                            <tr style="padding-bottom:20px;"
                                                                ng-show="notification.email">
                                                                <td style="width: 37%;">Email</td>
                                                                <td style="width: 3%;">:</td>
                                                                <td>{{notification.email}}</td>
                                                            </tr>

                                                            <tr style="padding-bottom:20px;"
                                                                ng-show="notification.pincode">
                                                                <td>PinCode</td>
                                                                <td>:</td>
                                                                <td>{{notification.pincode}}</td>
                                                            </tr>
                                                            <tr style="padding-bottom:20px;"
                                                                ng-show="notification.paymentStatus">
                                                                <td>Payment Status</td>
                                                                <td>:</td>
                                                                <td>{{notification.paymentStatus}}</td>
                                                            </tr>
                                                        </table>
                                                    </td>
                                                </tr>
                                            </table>
                                            <table class="border_info" style="width: 700px;" class="hi_table">
                                                <thead style="padding-top: 12px;">
                                                <h><b>Item Details</b></h>
                                                <tr>
                                                    <th>SL.No</th>
                                                    <th>Item Name</th>
                                                    <th>Tax</th>
                                                    <th>Unit Price</th>
                                                    <th>Quantity</th>
                                                    <th>Final Price</th>
                                                </tr>
                                                </thead>
                                                <tbody>
                                                <tr ng-repeat="item in notification.itemdetails">
                                                    <td>{{$index+1}}</td>
                                                    <td>{{item.item_name}}</td>
                                                    <td>
                                                        {{(item.item_final_price/item.item_quantity)-item.item_unit_price|number:2}}
                                                    </td>
                                                    <td>{{item.item_unit_price}}</td>
                                                    <td>{{item.item_quantity}}</td>
                                                    <td>{{item.item_final_price|number:2}}</td>
                                                </tr>
                                                </tbody>

                                            </table>
                                </table>

                                <br>
                                <h><b style="text-align: center">Instructions:</b><br></h>
                                <br>
                                <p>{{notification.instructions}}</p>
                    </table>
                    <table class="hi_tabled"
                           style="width: 700px;padding-bottom:10px;border-bottom: 1px solid black;margin-top: -70px !important;">
                        <br>
                        <h><b>Payment Details</b></h>
                        <tr style="text-align: right;margin-left: 10px">
                            <td>SubTotal</td>
                            <td>:</td>
                            <td>{{notification.subTotal|number:2}}</td>
                            <td>&nbsp;</td>
                        </tr>
                        <br>
                        <tr style="text-align: right;margin-left: 10px" ng-repeat="charge in notification.charges">
                            <td>{{charge.title}}</td>
                            <td>:</td>
                            <td>{{charge.value|number:2}}</td>
                            <td>&nbsp;</td>
                        </tr>
                        <br>
                        <tr style="text-align: right;margin-left: 10px">
                            <td>Total Charges</td>
                            <td>:</td>
                            <td>{{notification.itemCharge|number:2}}</td>
                            <td>&nbsp;</td>
                        </tr>
                        <br>
                        <tr style="text-align: right;margin-left: 10px">
                            <td>Discount</td>
                            <td>:</td>
                            <td>{{notification.discount|number:2}}</td>
                            <td>&nbsp;</td>
                        </tr>
                        <br>
                        <tr style="text-align: right;margin-left: 10px">
                            <td>Total Tax</td>
                            <td>:</td>
                            <td>{{notification.totalTaxes|number:2}}</td>
                            <td>&nbsp;</td>
                        </tr>
                        <br>
                        <tr style="text-align: right;margin-left: 10px">
                            <td>Order Total</td>
                            <td>:</td>
                            <td>{{notification.total|number:2}}</td>
                            <td>&nbsp;</td>
                        </tr>
                        <br>
                        <tr style="text-align: right">
                            <td>Payment Mode</td>
                            <td> :</td>
                            <td>{{notification.paymentmode}}</td>
                            <td>&nbsp;</td>
                        </tr>
                    </table>
                    <table class="border_info" class="hi_table">
                        <button style="float: right" type="button" data-dismiss="modal" class="btn btn-success">Close
                        </button>
                    </table>
                </div>
            </div>
        </div>
    </div>
