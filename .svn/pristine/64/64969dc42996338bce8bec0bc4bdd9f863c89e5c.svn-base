                                                    <img src="resource/images/favicon-16x16.png"
                                                         style="width:48px; height:48px; "/>
                                                    <p style="margin: 10px 0 0;color: dimgray;font-weight: 60; border: 1px solid;padding: 0 5px;">
                                                        {{notification.typeFlag}}
                                                    </p>
                                                </div>
                                                <div class="col-m`d-3 text-center"
                                                     ng-show="notification.fromCompname=='FoodPanda'||notification.fromCompname=='foodpanda'">
                                                    <img src="resource/images/foodpanda.png"
                                                         style="width:48px; height:48px; "/>
                                                    <p style="margin: 10px 0 0;color: dimgray;font-weight: 60; border: 1px solid;padding: 0 5px;">
                                                        {{notification.typeFlag}}
                                                    </p>
