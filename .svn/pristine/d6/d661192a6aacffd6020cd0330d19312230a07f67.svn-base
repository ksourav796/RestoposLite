                            <tr>
                                <th data-field="date" class="align_center">
                                    <div class="th-inner">Account Code</div>
                                </th>
                                <th data-field="formno" class="align_center">
                                    <div class="th-inner">Account Name</div>
                                </th>
                                <th data-field="ref" class="align_center">
                                    <div class="th-inner">Debit</div>
                                </th>
                                <th data-field="date" class="align_center">
                                    <div class="th-inner">Credit</div>
                                </th>

                            </tr>
                            </thead>
                            <tbody>
                            <tr>
                                <td></td>
                                <td>Opening Balance Difference:</td>
                                <td>{{trialbalancepojo.openingBalDiffDebitAmt}}</td>
                                <td>{{trialbalancepojo.openingBalDiffCreditAmt}}</td>
                            </tr>
                            <tr ng-repeat="tb in trialbalancepojo">
                                <td><strong>{{tb.desc}}</strong></td>
                                <td><strong><a href="javascript:void(0)"
                                               ng-click="getAccountName(tb.accName)">{{tb.accName}}</a></strong>
                                </td>
                                <td class="align_right">{{tb.debit | number:2}}</td>
                                <td class="align_right">{{tb.credit | number:2}}</td>
                            </tr>


                            </tbody>
                            <tfoot>
                            <tr>
                                <td>&nbsp;</td>
                                <td class="align_center"><strong>Total</strong></td>
                                <td class="align_right"><strong>{{trialbalancepojo.totalDebit | number:2}}</strong></td>
                                <td class="align_right"><strong>{{trialbalancepojo.totalCredit | number:2}}</strong>
                                </td>

                            </tr>

                            </tfoot>
                        </table>
                        <div id="printablediv">
                            <div class="header-background" ng-hide="true">
                                <fieldset>

                                </fieldset>
                                <table class="export-table" data-toggle="table" data-cache="false" data-height="299"
